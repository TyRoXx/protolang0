return function (assert)
{
	assert([])
	assert([] != [])
	assert([a = 0])
	assert([] != 0)
	assert([] != null)
	assert([] != function (){})

	{
		var t = []
		assert(t.a == null)
		t.a = 2
		assert(t.a == 2)
		assert(t["a"] == 2)
		assert(t.b == null)
		assert(t["b"] == null)
		t["b"] = 3
		assert(t.b == 3)
		assert(t["b"] == 3)
	}

	{
		var t = [a = 2]
		var q = [a = 2]
		assert(t != q)
		assert(t.a == 2)
		assert(t.b == null)
	}

	{
		var t = [f = function () { return 7 }]
		assert(t.f() == 7)
		assert(t["f"]() == 7)
	}
}
